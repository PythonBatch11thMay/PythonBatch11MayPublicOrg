#pattern
for i in range(1,n+1,1):
  print("*"*i)

#reverse pattern
for i in range(n,0,-1):
  print("*"*i)

#pattern3
for i in range(n,0,-1):
  string=(" "*(i-1))+("*"*(n-(i-1)))
  print(string)






